      name="TaskImporterWS"
      class="bibs.bibs.dataimporter.whoscored.TaskImporterWS"
      group="synapse.simple.quartz">
   <trigger count="1" interval="30"/>
   <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks"
             name="sequenceName"
             value="ImportReportMatches"/>
   <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks"
             name="incialMonth"
             value="Aug 2016"/>
   <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks"
             name="injectTo"
             value="sequence"/>
   <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks"
             name="cantMonth"
             value="1"/>
   <property xmlns:task="http://www.wso2.org/products/wso2commons/tasks"
             name="userAgent"
             value="Mozilla/5.0"/>
